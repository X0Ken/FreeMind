		<plugin_property name="load_file" value="true"/>
		<plugin_property name="create_html_linked_image" value="false"/>
	</plugin_action>
	<plugin_action documentation="%accessories/plugins/ExportWithXSLT_HTML3.properties_documentation" name="%accessories/plugins/ExportWithXSLT_HTML3.properties_name" base="freemind.extensions.ModeControllerHookAdapter" label="accessories/plugins/ExportWithXSLT_HTML3.properties" class_name="accessories.plugins.ExportWithXSLT">
